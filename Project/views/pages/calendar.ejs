<!-- <!DOCTYPE html> -->
<html>
 
<head>
  <meta charset="UTF-8">
  <title>HTML CSS JavaScript Calender | Webdevtrick.com</title>
    <link rel="stylesheet" href="../styles/style.css">
    <link rel="stylesheet" href="../styles/calendar.css">
</head>
<body>
    <div class="topnav">
        <a class="active" href="/">HOME</a>
        <a href="login">LOGIN</a>
        <a href="register">SIGN UP</a>
        <a href="about" style="float:right">ABOUT</a>
        <a href = "calendar style = float:right">CALENDER</a>
      </div>
      
    <div class="app">
        <button class="show-header"></button>
        <header>
            <nav>
                <ul>
                    <li>
                        <a href="#" data-selected>
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                <path d="M 6 5 L 18 5 Q 20 5, 20 7 L 20 19 Q 20 21, 18 21 L 6 21 Q 4 21, 4 19 L 4 7 Q 4 5, 6 5" />
                                <line x1="9" y1="3" x2="9" y2="7" />
                                <line x1="15" y1="3" x2="15" y2="7" />
                                <line x1="4" y1="11" x2="20" y2="11" />
                            </svg>
                        </a>
                    </li>
                    <li>
                        <a href="#">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                <polygon points="4 4 4 10 10 10 10 4" />
                                <polygon points="14 4 14 10 20 10 20 4" />
                                <polygon points="4 14 4 20 10 20 10 14" />
                                <polygon points="14 14 14 20 20 20 20 14" />
                            </svg>
                        </a>
                        <a href="#">
                            <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                                <circle cx="12" cy="12" r="7" />
                                <path d="M 4 6 L 6 4 M 20 6 L 18 4 M 17 17 L 20 20 M 7 17 L 4 20" />
                                <polyline points="12 9 12 12 14 14" />
                            </svg>
                        </a>
                    </li>
                </ul>
            </nav>
        </header>
        <div class="calendar">
            <div class="top-search">
                <svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 24 24" fill="none" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-linejoin="round">
                    <circle cx="14" cy="10" r="7" />
                    <line x1="4" y1="20" x2="9" y2="15" />
                </svg>
                <input type="search" placeholder="Search">
            </div>
            <h1>Calendar</h1>
            <div class="calendar-layout">
                <h2>November</h2>
                <!-- each of these <span> elements represents
                     one day, and the .null ones are for when
                     the month starts in the middle of the week
                -->
                <span class="null"></span>
                <span class="null"></span>
                <span class="null"></span>
                <span>1</span>
                <span>2</span>
                <span>3</span>
                <span>4</span>
                <span>5</span>
                <span>6</span>
                <span>7</span>
                <span>8</span>
                <span>9</span>
                <span>10</span>
                <span>11</span>
                <span>12</span>
                <span>13</span>
                <span data-selected>14</span>
                <span>15</span>
                <span>16</span>
                <span>17</span>
                <span>18</span>
                <span>19</span>
                <span>20</span>
                <span>21</span>
                <span>22</span>
                <span>23</span>
                <span>24</span>
                <span>25</span>
                <span>26</span>
                <span>27</span>
                <span>28</span>
                <span>29</span>
                <span>30</span>
            </div>
        </div>
        <div class="events">
            <h2>Wednesday, 14th November</h2>
            <ul class="events-list">
                <li>
                    <span class="time">10:30</span>
                    <span class="event">Meeting</span>
                </li>
                <li>
                    <span class="time">12:15</span>
                    <span class="event">Lunch</span>
                </li>
                <li>
                    <span class="time">14:45</span>
                    <span class="event">Interview</span>
                </li>
                <li>
                    <span class="time">15:20</span>
                    <span class="event">Have cake</span>
                </li>
            </ul>
        </div>
    </div>
</body>
 
</html>
